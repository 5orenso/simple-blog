{
  "name": "simple-blog",
  "description": "A very simple blog server. Written in Node.js with support for Markdown files, images, embedded content and all other stuff you would expect from a blog.",
  "version": "0.3.0",
  "homepage": "https://github.com/5orenso/simple-blog",
  "repository": {
    "type": "git",
    "url": "https://github.com/5orenso/simple-blog"
  },
  "bugs": {
    "url": "https://github.com/5orenso/simple-blog/issues"
  },
  "author": {
    "name": "Øistein Sørensen",
    "email": "sorenso@gmail.com"
  },
  "main": "app/server.js",
  "engines": {
    "node": ">=6.11"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || PORT=55445 npm run -s dev",
    "lint": "eslint ./index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch --onlyChanged",
    "build": "bash ./bin/build-release.sh",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-shorthand-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "buster": "^0.7.18",
    "buster-istanbul": "^0.1.15",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "grunt": "^1.0.2",
    "grunt-buster": "^0.4.2",
    "grunt-contrib-nodeunit": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-coveralls": "^1.0.1",
    "grunt-depcheck": "^0.1.3",
    "grunt-eslint": "^20.1.0",
    "grunt-jsdoc": "^2.2.1",
    "grunt-nodemon": "^0.4.2",
    "grunt-retire": "^1.0.7",
    "grunt-shell": "^2.1.0",
    "grunt-webpack": "^3.0.2",
    "html-md": "~3.0.2",
    "ink-docstrap": "^1.3.2",
    "jest": "^21.2.1",
    "jsdoc": "^3.5.5",
    "mockingoose": "^2.12.0",
    "request": "~2.83.0",
    "sinon": "^4.4.2",
    "time-grunt": "^1.4.0",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.0.1",
    "xml2js": "~0.4.19"
  },
  "keywords": [],
  "dependencies": {
    "body-parser": "^1.18.2",
    "client-sessions": "^0.8.0",
    "commander": "^2.14.1",
    "compression": "^1.7.2",
    "elasticsearch": "^14.1.0",
    "express": "^4.16.2",
    "express-fileupload": "^0.4.0",
    "express-jwt": "^5.3.0",
    "highlight.js": "^9.12.0",
    "imagemagick": "^0.1.3",
    "js-yaml": "^3.10.0",
    "jsonwebtoken": "^8.2.0",
    "lynx": "^0.2.0",
    "marked": "^0.3.17",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2",
    "mongoose": "^5.4.7",
    "morgan": "^1.9.0",
    "node-dogstatsd": "0.0.7",
    "rest": "^2.0.0",
    "sendgrid": "^5.2.3",
    "strftime": "^0.10.0",
    "swig": "^1.4.2",
    "underscore": "^1.8.3",
    "uuid": "^3.2.1",
    "walk": "^2.3.9",
    "web-push": "^3.3.0",
    "websequencediagrams": "0.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 10,
        "branches": 10,
        "functions": 10,
        "lines": 10
      }
    }
  }
}
