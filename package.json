    "elasticsearch": "^16.7.2",
    "exif": "^0.6.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.1",
    "express-jwt": "^5.3.1",
    "express-useragent": "^1.0.15",
    "face-api.js": "^0.19.0",
    "fast-type-check": "^1.3.2",
    "geo-lib": "^0.3.2",
