{
  "name": "simple-blog",
  "description": "A very simple blog server. Written in Node.js with support for Markdown files, images, embedded content and all other stuff you would expect from a blog.",
  "version": "0.3.0",
  "homepage": "https://github.com/5orenso/simple-blog",
  "repository": {
    "type": "git",
    "url": "https://github.com/5orenso/simple-blog"
  },
  "bugs": {
    "url": "https://github.com/5orenso/simple-blog/issues"
  },
  "author": {
    "name": "Øistein Sørensen",
    "email": "sorenso@gmail.com"
  },
  "main": "app/server.js",
  "engines": {
    "node": ">=8.11.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || PORT=55445 npm run -s dev",
    "lint": "eslint ./lib/**/*.js ./app/**/*.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch --onlyChanged",
    "build": "bash ./bin/build-release.sh",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@tensorflow-models/mobilenet": "^1.0.0",
    "@tensorflow-models/coco-ssd": "^1.0.0",
    "@tensorflow/tfjs": "^1.0.2",
    "@tensorflow/tfjs-core": "^1.0.2",
    "@tensorflow/tfjs-node": "^1.0.2",
    "buster": "^0.7.18",
    "buster-istanbul": "^0.1.15",
    "coveralls": "^3.0.2",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "grunt": "^1.0.3",
    "grunt-buster": "^0.4.2",
    "grunt-contrib-nodeunit": "^2.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-coveralls": "^2.0.0",
    "grunt-depcheck": "^0.1.3",
    "grunt-eslint": "^21.0.0",
    "grunt-jsdoc": "^2.3.0",
    "grunt-nodemon": "^0.4.2",
    "grunt-retire": "^1.0.7",
    "grunt-shell": "^3.0.1",
    "grunt-webpack": "^3.1.3",
    "html-md": "~3.0.2",
    "ink-docstrap": "^1.3.2",
    "jest": "^24.0.0",
    "jpeg-js": "^0.3.4",
    "jsdoc": "^3.5.5",
    "mockingoose": "^2.12.0",
    "node-gyp": "^3.8.0",
    "request": "~2.88.0",
    "sinon": "^7.2.3",
    "time-grunt": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "xml2js": "~0.4.19"
  },
  "keywords": [],
  "dependencies": {
    "body-parser": "^1.18.3",
    "client-sessions": "^0.8.0",
    "commander": "^2.19.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "elasticsearch": "^15.3.1",
    "exif": "^0.6.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.1",
    "express-jwt": "^5.3.1",
    "highlight.js": "^9.14.2",
    "imagemagick": "^0.1.3",
    "jest-runner-eslint": "^0.7.3",
    "js-yaml": "^3.12.1",
    "jsonwebtoken": "^8.4.0",
    "lynx": "^0.2.0",
    "marked": "^0.6.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.8",
    "morgan": "^1.9.1",
    "node-dogstatsd": "0.0.7",
    "rest": "^2.0.0",
    "sendgrid": "^5.2.3",
    "strftime": "^0.10.0",
    "swig": "^1.4.2",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "walk": "^2.3.14",
    "web-push": "^3.3.3",
    "websequencediagrams": "0.1.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 10,
        "branches": 10,
        "functions": 10,
        "lines": 10
      }
    }
  }
}
