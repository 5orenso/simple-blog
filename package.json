{
  "name": "simple-blog",
  "description": "A very simple blog server. Written in Node.js with support for Markdown files, images, embedded content and all other stuff you would expect from a blog.",
  "version": "0.3.0",
  "homepage": "https://github.com/5orenso/simple-blog",
  "repository": {
    "type": "git",
    "url": "https://github.com/5orenso/simple-blog"
  },
  "bugs": {
    "url": "https://github.com/5orenso/simple-blog/issues"
  },
  "author": {
    "name": "Øistein Sørensen",
    "email": "sorenso@gmail.com"
  },
  "main": "app/server.js",
  "engines": {
    "node": ">=8.11.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || PORT=55445 npm run -s dev",
    "lint": "eslint ./lib/**/*.js ./app/**/*.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch --onlyChanged",
    "build": "bash ./bin/build-release.sh",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "buster": "^0.7.18",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "grunt": "^1.0.4",
    "grunt-buster": "^0.4.2",
    "grunt-contrib-nodeunit": "^2.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-coveralls": "^2.0.0",
    "grunt-depcheck": "^0.1.3",
    "grunt-eslint": "^21.0.0",
    "grunt-jsdoc": "^2.3.0",
    "grunt-nodemon": "^0.4.2",
    "grunt-retire": "^1.0.7",
    "grunt-shell": "^3.0.1",
    "grunt-webpack": "^3.1.3",
    "ink-docstrap": "^1.3.2",
    "jest": "^24.5.0",
    "jsdoc": "^3.5.5",
    "mockingoose": "^2.12.0",
    "node-gyp": "^3.8.0",
    "request": "^2.88.2",
    "sinon": "^7.3.1",
    "time-grunt": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "xml2js": "~0.4.19"
  },
  "keywords": [],
  "dependencies": {
    "@google-cloud/translate": "^6.2.7",
    "@tmcw/togeojson": "^4.5.0",
    "aws-sdk": "^2.522.0",
    "body-parser": "^1.18.3",
    "client-sessions": "^0.8.0",
    "colorthief": "^2.3.2",
