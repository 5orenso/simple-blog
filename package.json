    "swig": "^1.4.2",
    "underscore": "^1.9.1",
    "uuid": "^3.4.0",
    "walk": "^2.3.14",
    "web-push": "^3.3.3",
    "websequencediagrams": "1.0.0",
    "websocket": "^1.0.34",
    "xml-js": "^1.6.11",
    "xmldom": "^0.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 10,
        "branches": 10,
        "functions": 10,
        "lines": 10
      }
    }
  }
}
